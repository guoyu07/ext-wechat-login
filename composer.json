{
    "name": "notadd/wechat-login",
    "description": "Notadd Extension for wechat-login",
    "type": "notadd-extension",
    "keywords": ["notadd", "wechat", "extension"],
    "homepage": "https://notadd.com",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "AllenGu",
            "email": "674397601@qq.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Notadd\\WechatLogin\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "replace": {
        "guzzlehttp/guzzle": "10.5.*",
        "guzzlehttp/promises": "10.5.*",
        "guzzlehttp/psr7": "10.5.*",
        "monolog/monolog": "10.5.*",
        "psr/container": "10.5.*",
        "psr/http-message": "10.5.*",
        "psr/log": "10.5.*",
        "symfony/http-foundation": "10.5.*",
        "symfony/polyfill-mbstring": "10.5.*",
        "symfony/psr-http-message-bridge": "10.5.*"
    },
    "require": {
        "php": ">=7.0",
        "overtrue/laravel-socialite": "~1.0",
        "overtrue/wechat": "~3.1",
        "bacon/bacon-qr-code": "~1.0"
    },
    "require-dev": {
        "notadd/installers": "0.14.*",
        "notadd/testing": "0.4.*",
        "phpunit/phpunit": "~6.0"
    }
}
